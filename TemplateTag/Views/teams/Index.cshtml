@model IEnumerable<VGTA.Data.Models.team>

@{
    ViewBag.Title = "Index";
}

        <!-- 5edma -->
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.max_members)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.name)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.max_members)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.name)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.id_team }) <img src="~/Content/images/edit.png" style="width:50px;height:50px"></img>|
                        @Html.ActionLink("Details", "Details", new { id = item.id_team }) <img src="~/Content/images/details.png" style="width:50px;height:50px"></img> |
                        @Html.ActionLink("Delete", "Delete", new { id = item.id_team })<img src="~/Content/images/delete.png" style="width:50px;height:50px"></img>


                    </td>
                </tr>
            }

        </table>

